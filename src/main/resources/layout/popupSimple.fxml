<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox id="messageBody" fx:id="messageBody" prefWidth="360.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Pane id="alertPane" fx:id="alertPane" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
            <VBox HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
               <children>
                  <Label id="messageTitle" fx:id="messageTitle" text="Lorem Ipsum" wrapText="true" VBox.vgrow="ALWAYS">
                     <font>
                        <Font name="Arial" size="16.0" />
                     </font>
                  </Label>
                  <Label id="messageContext" fx:id="messageContext" layoutX="10.0" layoutY="10.0" text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec quis dapibus nisi.}" wrapText="true" VBox.vgrow="ALWAYS">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <Button id="buttonCloseMessage" fx:id="buttonCloseMessage" mnemonicParsing="false" prefHeight="0.0" prefWidth="1.0" text="X" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
